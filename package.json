{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "manual-dev-env-setup": "dotenv -e .env -e .env.development -e .env.local -e .env.development.local",
    "manual-prod-env-setup": "dotenv -e .env -e .env.production -e .env.local -e .env.production.local",
    "postinstall": "prisma generate",
    "preinstall": "dotenv -e .env -e .env.local -- ts-node --project=./tsconfig.json src/application/support/enforce-node-version.ts",
    "preadd": "dotenv -e .env -e .env.local -- ts-node --project=./tsconfig.json src/application/support/enforce-node-version.ts && add",
    "analyze-build": "ANALYZE_BUILD=true pnpm build",
    "build": "next build",
    "build:local": "rm -rf ./.next && next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint && stylelint \"./src/**/*.{scss,css}\"",
    "preeslint-fix": "pnpm setup",
    "eslint-fix": "npx eslint . --fix",
    "test": "jest --watchAll  --silent=false",
    "test:ci": "jest --ci",
    "tsc": "tsc --project ./tsconfig.json",
    "tsc:watch": "tsc -w",
    "create-migrations": "pnpm manual-dev-env-setup -- prisma migrate dev --create-only",
    "migrate-dev": "pnpm manual-dev-env-setup -- cross-env NODE_ENV=development prisma migrate dev",
    "migrate-reset": "pnpm manual-dev-env-setup -- prisma migrate reset",
    "migrate-reset-prod": "pnpm manual-prod-env-setup -- prisma migrate reset",
    "pushdb": "prisma db push",
    "generate-client": "prisma generate",
    "seeddb": "pnpm manual-dev-env-setup -- prisma db seed",
    "jsonify-fixtures": "pnpm manual-dev-env-setup -- cross-env NODE_ENV=development npx tsx --tsconfig ./tsconfig.json src/prisma/jsonify.ts",
    "pullenv": "vercel env pull ./.env --environment=development"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.7",
    "@clerk/nextjs": "^4.29.4",
    "@clerk/themes": "^1.7.9",
    "@floating-ui/react": "^0.26.9",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@mantine/core": "^7.4.2",
    "@nextui-org/skeleton": "^2.0.24",
    "@nivo/bar": "^0.84.0",
    "@prisma/client": "5.8.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "chart.js": "^4.4.1",
    "clsx": "^2.1.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "framer-motion": "^10.18.0",
    "lodash.clamp": "^4.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniq": "^4.5.0",
    "luxon": "^3.4.4",
    "mantine-datatable": "^7.5.1",
    "next": "14.0.4",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.3",
    "react-toastify": "^10.0.4",
    "sass": "^1.69.7",
    "semver": "^7.5.4",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "swr": "^2.2.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@types/eslint": "^8.56.1",
    "@types/jest": "^29.5.11",
    "@types/lodash.clamp": "^4.0.9",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.difference": "^4.5.9",
    "@types/lodash.intersection": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.pick": "^4.4.9",
    "@types/lodash.uniq": "^4.5.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semver": "^7.5.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "extend-expect": "link:@testing-library/jest-dom/extend-expect",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-runner-eslint": "^2.1.2",
    "jest-runner-prettier": "^1.0.0",
    "jest-runner-stylelint": "^2.3.7",
    "postcss": "^8.4.33",
    "prisma": "^5.8.0",
    "stylelint": "~15.11.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.0.0",
    "stylelint-webpack-plugin": "^4.1.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "utility-types": "^3.10.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "dotenv -e .env -e .env.development -e .env.local -e .env.development.local cross-env NODE_ENV=development npx tsx --tsconfig ./tsconfig.json src/prisma/seed.ts"
  }
}
